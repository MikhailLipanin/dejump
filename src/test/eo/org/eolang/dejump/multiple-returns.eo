[] > multiple_returns
  [x] > abs
    goto > @
      [g]
        seq > @
          if.
            x.gt 0
            g.forward x
            TRUE
          g.forward
            -1.times x
  if. > @
    eq.
      QQ.txt.sprintf "%d equals to %d\n"
        abs 228
        abs 228
      "228 equals to 228\n"
    nop
    FALSE
